<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.102-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <TabPane prefHeight="502.0" prefWidth="635.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="ServerTab" text="Server">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="ServerHostField" layoutX="108.0" layoutY="28.0" text="127.0.0.1">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </TextField>
                        <Label fx:id="ServerHostLabel" layoutX="37.0" layoutY="34.0" text="Host">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <Label fx:id="ServerPortLabel" layoutX="39.0" layoutY="69.0" text="Port">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField fx:id="ServerPortField" layoutX="108.0" layoutY="63.0" text="9999">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </TextField>
                        <Label fx:id="ServerSizeLabel" layoutX="39.0" layoutY="105.0" text="Fragment size">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField fx:id="ServerSizeField" editable="false" layoutX="188.0" layoutY="99.0" prefHeight="32.0" prefWidth="130.0" text="8">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </TextField>
                        <Button fx:id="ServerListenButton" layoutX="248.0" layoutY="168.0" mnemonicParsing="false" onAction="#handleServerListenButton" text="Listen">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Button>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="ClientTab" text="Client">
              <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                      <children>
                          <TextField fx:id="ClientHostField" layoutX="108.0" layoutY="28.0" text="127.0.0.1">
                              <font>
                                  <Font size="16.0" />
                              </font>
                          </TextField>
                          <Label fx:id="ClientHostLabel" layoutX="37.0" layoutY="34.0" text="Host">
                              <font>
                                  <Font size="16.0" />
                              </font>
                          </Label>
                          <Label fx:id="ClientPortLabel" layoutX="39.0" layoutY="69.0" text="Port">
                              <font>
                                  <Font size="16.0" />
                              </font>
                          </Label>
                          <TextField fx:id="ClientPortField" layoutX="108.0" layoutY="63.0" text="9999">
                              <font>
                                  <Font size="16.0" />
                              </font>
                          </TextField>
                          <Label fx:id="ClientSizeLabel" layoutX="39.0" layoutY="105.0" text="Fragment size">
                              <font>
                                  <Font size="16.0" />
                              </font>
                          </Label>
                          <TextField fx:id="ClientSizeField" layoutX="188.0" layoutY="99.0" prefHeight="32.0" prefWidth="130.0" text="8">
                              <font>
                                  <Font size="16.0" />
                              </font>
                          </TextField>
                          <Button fx:id="ClientSendButton" layoutX="536.0" layoutY="135.0" mnemonicParsing="false" onAction="#handleClientSendButton" text="Send">
                              <font>
                                  <Font size="16.0" />
                              </font>
                          </Button>
                        <Button fx:id="ClientConnectButton" layoutX="109.0" layoutY="135.0" mnemonicParsing="false" text="Connect">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Button>
                        <Button fx:id="ClientDisconnectButton" layoutX="207.0" layoutY="135.0" mnemonicParsing="false" text="Disconnect">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Button>
                        <TextArea fx:id="ClientMessageField" layoutX="37.0" layoutY="185.0" prefHeight="259.0" prefWidth="562.0">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </TextArea>
                        <TextField fx:id="ClientFileField" editable="false" layoutX="323.0" layoutY="99.0" prefHeight="32.0" prefWidth="277.0" promptText="Choose file...">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </TextField>
                        <Button fx:id="ClientFileButton" layoutX="322.0" layoutY="135.0" mnemonicParsing="false" onAction="#handleClientFileButton" text="Choose file...">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Button>
                          <Label fx:id="ClientChecksumLabel" layoutX="322.0" layoutY="34.0" text="Bad checksum">
                              <font>
                                  <Font size="16.0" />
                              </font>
                          </Label>
                          <CheckBox fx:id="ClientChecksumBox" alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutX="447.0" layoutY="33.0" mnemonicParsing="false" nodeOrientation="RIGHT_TO_LEFT" prefHeight="18.0" prefWidth="11.0">
                              <font>
                                  <Font size="16.0" />
                              </font></CheckBox>
                      </children>
                  </AnchorPane>
              </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</GridPane>
